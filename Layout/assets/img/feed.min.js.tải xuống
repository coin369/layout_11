!function(u){feed=function(e){var d=u.extend({url:"blog",feedElement:"#feed",elementType:"dl"},e);function r(e){var t=new Date(e),n=t.getFullYear(),i=("00"+(t.getMonth()+1)).slice(-2),l=("00"+t.getDate()).slice(-2);t.getDay(),t.getHours(),t.getMinutes(),t.getSeconds();return n+"."+i+"."+l}u.ajax({url:d.url+"/feed",type:"get",dataType:"xml",timeout:5e3,success:function(e,t){if("success"===t){var n,l=0,a=[];if(u(e).find("item").each(function(){a[l]={},u(this).children().each(function(){for(var e in n=u(this)[0].nodeName,a[l][n]={},attributes=u(this)[0].attributes,attributes)a[l][n][attributes[e].name]=attributes[e].value;a[l][n].text=u(this).text()}),l++}),"dl"==d.elementType)for(i in a)u(d.feedElement).append("<dl><dt>"+r(a[i].pubDate.text)+'</dt><dd><a href="'+a[i].link.text+'">'+a[i].title.text+"</a></dd></dl>");else for(i in u(d.feedElement).wrapInner("<ul></ul>"),a)u(d.feedElement).find("ul").append('<li><a href="'+a[i].link.text+'">'+a[i].title.text+"</a></li>");u(d.feedElement).mCustomScrollbar({scrollInertia:300,advanced:{updateOnContentResize:!0}})}}})}}(jQuery);